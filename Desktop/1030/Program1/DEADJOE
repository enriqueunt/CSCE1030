/*
*Programer: Enrique Torres (et0132@unt.edu)
*Comapany Name: WE PAINT BEDROOMS
*Course: CSE 1030 Computer Science 1
*Due Date: 10/18/2012
*Description: This program's purpose is to calculate the amount of
              paint needed to paint a room (including the room's ceiling), 
              also to worry about not paiting the windows or doors in the room.
*/

#include <stdio.h> // common input/output operations

int main()
{
 int height, walla, wallb, wallc, walld, doors, windows;
 int door = 26;
 int gallon = 185;
 double window = 6.5;
 double Totalsquarefootage;
 double Gallons;
 int TotalGallons;
 
 
 // prompt the user for input
 printf ("Please enter room height: ");
 scanf ("%d",&height);
 
 printf ("Please enter wall length a: ");
 scanf ("%d",&walla);
 
 printf ("Please enter wall length b: ");
 scanf ("%d",&wallb);
 
 printf ("Please enter wall length c: ");
 scanf ("%d",&wallc);
 
 printf ("Please enter wall lenght d: ");
 scanf ("%d",&walld);
 
 printf ("Please enter number of windows: ");
 scanf ("%d",&windows);
 
 printf ("Please enter number of doors: ");
 scanf ("%d",&doors);
 
 Totalsquarefootage = height * (walla + wallb + wallc + walld) + (wallc * walld) + ((wallb - wallc) * height) + ((walld - walla) * height) + (wallb - wallc) * (walld - walla) - ((windows * window) + (doors * door));
 
 printf ("The square footage is %.1lf\n",Totalsquarefootage);
  
 Gallons = Totalsquarefootage / gallon;
 
 printf ("Gallons %.2lf\n",Gallons);
 
 printf ("Total gallons of paint to purchase %
 
 
 return 0; //return to Operating System
} 
  
     
   
   
   
